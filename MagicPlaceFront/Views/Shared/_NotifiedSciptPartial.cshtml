
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

@*y aqui Haremos uso de Temp Data  los cuales son datos que uno utiliza y se borran en el momento n el que
se desplegan*@



@if (TempData["isSucces"] != null)
{
    <script>

        Swal.fire({
            position: 'top-end',
            icon: 'success',
            title: '@TempData["isSucces"]',
            showConfirmButton: false,
            timer: 1500
        })

    </script>



}

@if (TempData["error"] != null)
{
    <script>

        Swal.fire({
            position: 'top-end',
            icon: 'error',
            title: '@TempData["error"]',
            showConfirmButton: false,
            timer: 1500
        })

    </script>



}
